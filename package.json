{
  "name": "octo-exam",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite & npm run style:watch",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint:check": "npx prettier --check .",
    "lint:format": "npx prettier --write .",
    "lint:fix": "eslint --ext .tsx,.ts,.js,.vue --fix ./src --fix",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "prepare": "husky install",
    "style:watch": "./node_modules/.bin/typed-scss-modules src --watch --ignore \"**/index.scss\""
  },
  "dependencies": {
    "axios": "^0.25.0",
    "element-plus": "^2.0.1",
    "pinia": "^2.0.11",
    "vue": "^3.2.25",
    "vue-query": "^1.19.0",
    "vue-router": "4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@element-plus/icons-vue": "^1.1.1",
    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "autoprefixer": "^10.4.2",
    "commitlint-config-cz": "^0.13.3",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "postcss": "^8.4.6",
    "prettier": "2.5.1",
    "sass": "^1.49.7",
    "tailwindcss": "^3.0.19",
    "typed-scss-modules": "^5.0.0",
    "typescript": "~4.5.2",
    "unplugin-element-plus": "^0.2.0",
    "unplugin-icons": "^0.13.3",
    "vite": "^2.8.0",
    "vue-tsc": "^0.29.8"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,ts,tsx,vue}": "eslint --ext .tsx,.ts,.js,.vue --fix ./src"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  }
}
